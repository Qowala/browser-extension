{
  "manifest_version": 2,
  "name": "Qowala",
  "description": "This extension helps you to overcome your Web addiction",
  "version": "0.3.0",
  "applications": {
    "gecko": {
      "id": "{2cd8f8d0-b9a5-4a68-8ad4-13d001178858}"
    }
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "favicon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background-script.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
