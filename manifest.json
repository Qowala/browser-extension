{
  "manifest_version": 2,

  "name": "Qowala",
  "description": "This extension helps you to overcome your Web addiction",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "storage"
  ],
  "background": {
    "scripts": ["background-script.js"]
  }
}
